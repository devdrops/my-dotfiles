--langdef=JSON
--langmap=JSON:.json

--langdef=TOML
--langmap=TOML:.toml

--langdef=YAML
--langmap=YAML:.yaml.yml

--langdef=Dockerfile
--langmap=Dockerfile:([Dd]ockerfile)

--langdef=Textfile
--langmap=Textfile:.txt.doc.docx.rtf

--langmap=Vim:(vimrc).vim

--langdef=Go
--langmap=Go:(go.mod).go
--regex-Go=/func([ \t]+\([^)]+\))?[ \t]+([a-zA-Z0-9_]+)/\2/d,func/
--regex-Go=/var[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,var/
--regex-Go=/type[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,type/

--langdef=TypeScript
--langmap=TypeScript:.ts

--langdef=TypeScript_React
--langmap=TypeScript_React:.tsx

--langdef=Sass
--langmap=Sass:.scss

--langdef=terminfo
--langmap=terminfo:.terminfo

--langdef=BasicTemplate
--langmap=BasicTemplate:.tpl

--langdef=Conf
--langmap=Conf:.conf

--regex-php=/^[ \t]*trait[ \t]+([a-z0_9_]+)/\1/t,traits/i
--regex-php=/^[ \t]*enum[ \t]+([a-z0_9_]+)/\1/e,enums/i
