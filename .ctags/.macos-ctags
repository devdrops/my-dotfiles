--recurse=yes

--verbose=yes

--totals=yes

--exclude=.idea
--exclude=.git
--exclude=.github
--exclude=.gitkeep
--exclude=.gitignore
--exclude=.svn
--exclude=node_modules
--exclude=build
--exclude=vendor/*
--exclude=\*.html
--exclude=\*.out
--exclude=\*.yaml
--exclude=\*.yml
--exclude=\*.json
--exclude=\*.xml
--exclude=\*.md
--exclude=\*.markdown
--exclude=\*.log
--exclude=LICENSE
--exclude=.dockerignore
--exclude=Makefile
--exclude=makefile
--exclude=sonar-project.properties

--langdef=Go
--langmap=Go:.go
--regex-Go=/func([ \t]+\([^)]+\))?[ \t]+([a-zA-Z0-9_]+)/\2/d,func/
--regex-Go=/var[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,var/
--regex-Go=/type[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/d,type/
